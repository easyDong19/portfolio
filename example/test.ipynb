    "from functools import reduce\n",
    "\n",
    "lists = [3,2,1,-3,-2]\n",
    "def negative(acc, curr):\n",
    "    if curr <0:\n",
    "        acc += 1\n",
    "    return acc\n",
    "reduce(negative,lists,0)\n"
   ],
